<?xml version="1.0"?>
<root>
  <item>
    <name>Swap ( and à</name>
    <identifier>private.swap_opening_parenthesis_and_a</identifier>
    <autogen>
        __KeyToKey__
        KeyCode::KEY_0, ModifierFlag::NONE,
        KeyCode::KEY_5, ModifierFlag::NONE,
    </autogen>
    <autogen>
        __KeyToKey__
        KeyCode::KEY_5, ModifierFlag::NONE,
        KeyCode::KEY_0, ModifierFlag::NONE,
    </autogen>
    <name>Swap ( and à continued</name>
    <identifier>private.swap_opening_parenthesis_and_a_c</identifier>
    <autogen>
        __KeyToKey__
        KeyCode::KEY_0, ModifierFlag::OPTION_L, ModifierFlag::OPTION_L,
        KeyCode::KEY_5, ModifierFlag::OPTION_L, ModifierFlag::OPTION_L,
    </autogen>
    <autogen>
        __KeyToKey__
        KeyCode::KEY_5, ModifierFlag::OPTION_L, ModifierFlag::OPTION_L,
        KeyCode::KEY_0, ModifierFlag::OPTION_L, ModifierFlag::OPTION_L,
    </autogen>
    <name>Swap ( and à finish</name>
    <identifier>private.swap_opening_parenthesis_and_a_f</identifier>
    <autogen>
        __KeyToKey__
        KeyCode::KEY_0, ModifierFlag::OPTION_L,
        KeyCode::KEY_5, ModifierFlag::OPTION_L,
    </autogen>
    <autogen>
        __KeyToKey__
        KeyCode::KEY_5, ModifierFlag::OPTION_L,
        KeyCode::KEY_0, ModifierFlag::OPTION_L,
    </autogen>

    <name>Map CTRL+J to ENTER</name>
    <identifier>private.map_ctrl_j_to_enter</identifier>
    <autogen>
        __KeyToKey__
        KeyCode::J, ModifierFlag::CONTROL_L,
        KeyCode::ENTER
    </autogen>
    <name>Map CTRL+F to _</name>
    <identifier>private.ctrl_f_to_underscore</identifier>
    <autogen>
        __KeyToKey__
        KeyCode::F, ModifierFlag::CONTROL_L,
        KeyCode::EQUAL, ModifierFlag::SHIFT_L,
    </autogen>
  </item>


